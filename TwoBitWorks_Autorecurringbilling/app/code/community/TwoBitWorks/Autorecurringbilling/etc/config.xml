<?xml version="1.0"?>
<config>
      <modules>
        <TwoBitWorks_Autorecurringbilling>
          <version>0.1.0</version>
        </TwoBitWorks_Autorecurringbilling>
      </modules>
    <frontend>
      <routers>
        <autorecurringbilling>
          <use>standard</use>
         <args>
           <module>TwoBitWorks_Autorecurringbilling</module>
           <frontName>autorecurringbilling</frontName>
         </args>
        </autorecurringbilling>
      </routers>
	  <layout>
          <updates>
              <autorecurringbilling>
                  <file>autorecurringbilling.xml</file>
              </autorecurringbilling>
          </updates>
      </layout>
    </frontend>
   <global>   
      <blocks>   
         <autorecurringbilling><class>TwoBitWorks_Autorecurringbilling_Block</class></autorecurringbilling>   
      </blocks>   
   </global>   
</config> 